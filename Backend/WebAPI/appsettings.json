{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=authapp.db"
  },
  "JwtSettings": {
    "SecretKey": "BiharTeacherPortal2024SecretKeyForJWTTokenGeneration123456789",
    "Issuer": "AuthApp",
    "Audience": "AuthAppUsers"
  },
  "AzureSearch": {
    "ServiceEndpoint": "https://bihar-teacher-search.search.windows.net",
    "ApiKey": "__AZURE_SEARCH_API_KEY__"
  },
  "AzureStorage": {
    "ConnectionString": "__AZURE_STORAGE_CONNECTION_STRING__",
    "ContainerName": "teacher-documents"
  },
  "AzureEmail": {
    "ConnectionString": "__AZURE_COMMUNICATION_EMAIL_CONNECTION_STRING__",
    "SenderEmail": "DoNotReply@biharteacherportal.com",
    "SenderName": "Bihar Teacher Portal"
  },
  "KeyVault": {
    "VaultUri": "__KEY_VAULT_URI__"
  },
  "Paytm": {
    "Environment": "Staging",
    "MerchantId": "__PAYTM_MID__",
    "MerchantKey": "__PAYTM_MERCHANT_KEY__",
    "Website": "DEFAULT",
    "IndustryType": "Retail",
    "ChannelId": "WEB",
    "CallbackUrl": "__PAYTM_CALLBACK_URL__",
    "PremiumAmountInPaise": 0
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}